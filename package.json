{
	"name": "marlin-staking",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"test": "npm run test:integration && npm run test:unit",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit:watch": "vitest",
		"test:unit": "vitest run",
		"testcoverage:unit:old": "vitest run --coverage",
		"testcoverage:unit": "vitest run --reporter=junit --coverage.reporter=json-summary --coverage.reporter=json",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"update": "ncu -u && npm i",
		"prepare": "node .husky/install.mjs",
		"test:integration": "synpress && playwright test",
		"test:integration:ui": "synpress && playwright test --ui",
		"synpress-test": "rm -rf .cache-synpress && synpress && playwright test --ui",
		"cleanup": "lilmolly",
		"test:ui": "vitest --ui",
		"test:integration:basic": "npx playwright test test/e2e/walletNotConnected"
	},
	"devDependencies": {
		"@playpilot/svelte-htm": "^1.0.1",
		"@playwright/test": "^1.44.0",
		"@sveltejs/adapter-static": "^3.0.2",
		"@sveltejs/kit": "^2.5.18",
		"@synthetixio/synpress": "^4.0.0-alpha.7",
		"@testing-library/svelte": "^5.2.0",
		"@types/node": "^20.14.11",
		"@typescript-eslint/eslint-plugin": "^6.5.0",
		"@typescript-eslint/parser": "^6.5.0",
		"@vitest/coverage-v8": "^2.0.3",
		"@vitest/ui": "^2.0.3",
		"autoprefixer": "^10.4.15",
		"browser": "^0.2.6",
		"dotenv": "^16.4.4",
		"eslint": "^8.48.0",
		"eslint-config-prettier": "^9.0.0",
		"eslint-plugin-svelte": "^2.33.0",
		"htm": "^3.1.1",
		"husky": "^9.1.1",
		"jsdom": "^24.1.0",
		"lilmolly": "^1.3.7",
		"lint-staged": "^15.2.7",
		"npm-check-updates": "^16.14.20",
		"postcss": "^8.4.39",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"prettier-plugin-tailwindcss": "^0.5.6",
		"process": "^0.11.10",
		"serve": "^14.2.3",
		"svelte": "^4.2.18",
		"svelte-check": "^3.8.4",
		"tailwindcss": "^3.4.6",
		"tslib": "^2.6.3",
		"typescript": "^5.5.3",
		"vite": "^5.3.4",
		"vitest": "^2.0.3"
	},
	"type": "module",
	"dependencies": {
		"@web3-onboard/coinbase": "^2.4.1",
		"@web3-onboard/core": "^2.22.2",
		"@web3-onboard/gnosis": "^2.3.1",
		"@web3-onboard/infinity-wallet": "^2.1.1",
		"@web3-onboard/injected-wallets": "^2.11.1",
		"@web3-onboard/ledger": "^2.7.1",
		"@web3-onboard/trezor": "^2.4.6",
		"@web3-onboard/walletconnect": "^2.6.1",
		"assert": "^2.1.0",
		"buffer": "^6.0.3",
		"clsx": "^2.1.1",
		"crypto-browserify": "^3.12.0",
		"daisyui": "^4.12.10",
		"ethers": "^6.13.1",
		"stream-browserify": "^3.0.0",
		"svelte-awesome": "^3.3.1",
		"tailwind-merge": "^2.4.0"
	}
}
